+string exec_cmd(string cmd) {
+    char buffer[128];
+    string result = "";
+    FILE* pipe = popen(cmd.c_str(), "r");
+    if (!pipe) {
+        cerr << "Error: popen() failed!" << endl;
+        exit(1);
+    }
+    while (fgets(buffer, sizeof(buffer), pipe) != nullptr) {
+        result += buffer;
+    }
+    pclose(pipe);
+    return result;
+}
